echo "installing daylily CLI"
brew install conda

conda init
conda init zsh
source ~/.zshrc
conda activate

conda install -y -c conda-forge mamba
mamba create -y -n DAYCLI -c conda-forge python parallel
conda activate DAYCLI
pip install -y aws-parallelcluster==3.5.0 awscli

echo " "
echo " "
