#!/usr/bin/env bash



if [[ "$1" == "b37" ]]; then
    echo "Setting genome to b37"
    export DAY_GENOME_BUILD="b37"
elif [[ "$1" == "hg38" ]]; then
    echo "Setting genome to hg38"
    export DAY_GENOME_BUILD="hg38"
else
    unset DAY_GENOME_BUILD
    echo "Error: Invalid genome build. Only 'b37' or 'hg38' currently supported."
    return 3
fi